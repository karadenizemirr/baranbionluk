    <div class="footer-top">
      <div class="container">
        <div class="inner">
          <div class="row">
            <div class="col-lg-3">
              <div class="footer-item footer-about">
                <div class="logo">
                  <a href="index.html">
                    <h1>
                      <span class="text-primary">mavi</span>Charge
                    </h1>
                  </a>
                </div>
